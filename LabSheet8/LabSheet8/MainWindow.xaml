<Window x:Class="LabSheet8.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LabSheet8"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        Loaded="Window_Loaded">

    <Grid>
        <ListBox x:Name="lbxTeams" HorizontalAlignment="Left" Height="264" Margin="116,56,0,0" VerticalAlignment="Top" Width="164"/>
        <ListBox x:Name="lbxPlayers" HorizontalAlignment="Left" Height="264" Margin="507,58,0,0" VerticalAlignment="Top" Width="147" RenderTransformOrigin="0.5,0.5">
            <ListBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleY="-0.919"/>
                    <RotateTransform/>
                    <TranslateTransform Y="-0.802"/>
                </TransformGroup>
            </ListBox.RenderTransform>
        </ListBox>

    </Grid>
</Window>
